{
    "name": "vscodefitnesse",
    "displayName": "VSCodeFitNesse",
    "description": "Extension used to format FitNesse wiki from Visual Studio Code.",
    "version": "0.0.1",
    "publisher": "chrisotto",
    "engines": {
        "vscode": "^1.18.0"
    },
    "license": "MIT",
    "keywords": [
        "fitnesse",
        "wiki",
        "formatter"
    ],
    "repository": {
        "type": "git",
        "url": "https://github.com/chrisotto6/VSCodeFitNesse.git"
    },
    "homepage": "https://github.com/chrisotto6/VSCodeFitNesse",
    "bugs": {
        "url": "https://github.com/chrisotto6/VSCodeFitNesse/issues"
    },
    "categories": [
        "Other"
    ],
    "activationEvents": [
        "onCommand:extension.sayHello"
    ],
    "main": "./out/extension",
    "contributes": {
        "commands": [
            {
                "command": "extension.sayHello",
                "title": "VS Code FitNesse"
            }
        ],
        "keybindings": [
            {
                "key": "alt+f",
                "command": "extension.sayHello"
            }
        ]
    },
    "scripts": {
        "vscode:prepublish": "npm run compile",
        "compile": "tsc -p ./",
        "watch": "tsc -watch -p ./",
        "postinstall": "node ./node_modules/vscode/bin/install",
        "test": "npm run compile && node ./node_modules/vscode/bin/test"
    },
    "devDependencies": {
        "typescript": "^2.6.1",
        "vscode": "^1.1.6",
        "@types/node": "^7.0.43",
        "@types/mocha": "^2.2.42"
    }
}
